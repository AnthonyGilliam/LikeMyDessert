<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>
        <link href="@Url.Content("~/Content/css/jquery-ui-1.8.18.custom.css")" rel="stylesheet" type="text/css" />
	    <link href="@Url.Content("~/Content/css/Global.min.css")" rel="stylesheet" type="text/css" />
	    <link href="@Url.Content("~/Content/css/Header.min.css")" rel="Stylesheet" type="text/css" />
        @RenderSection("Styles", false)
    </head>

    <body>
        @Html.Partial("_Header")

        <div id="content">
            @RenderBody()
        </div>

        <footer></footer>

        <script src="@Url.Content("~/Scripts/common/less-1.1.3.min.js")" type="text/javascript"></script>
        <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.6.js" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery/modernizr-1.7.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery/jquery-ui-1.8.18.custom.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery/jquery.form.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery/jquery.validate.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/global.js")" type="text/javascript"></script>
        @RenderSection("Scripts", false)
    </body>
</html>
